{
  "name": "api-key-aggregetor",
  "displayName": "api-key-aggregetor",
  "description": "gemini api key聚合",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.97.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:api-key-aggregetor.runserver",
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "api-key-aggregetor.runserver",
        "title": "Run Server"
      }
    ],
    "configuration": {
      "title": "Gemini API Key Aggregator Configuration",
      "properties": {
        "geminiAggregator.apiKeys": {
          "type": "array",
          "default": [],
          "description": "List of Gemini API keys to be used by the aggregator."
        },
        "geminiAggregator.port": {
          "type": "number",
          "default": 3145,
          "description": "List of Gemini API keys to be used by the aggregator."
        }
      }
    
    }
  },

  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/express": "^5.0.1",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.97.0",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.23.0",
    "typescript": "^5.8.2"
  },
  "dependencies": {
    "@google/generative-ai": "^0.15.0",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "pino": "^9.2.0",
    "pino-http": "^10.1.0",
    "pino-pretty": "^11.2.1"
  }
}
